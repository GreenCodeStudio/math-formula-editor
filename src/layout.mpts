<div>
    <div class="nodes">{{nodes}}
    <:foreach collection=nodes item=node>
        <div class="node" data-code=node.code>
            {{node.name}}
        </div>
    </:foreach>
    </div>
    <div class="operations">
        <:foreach collection=operations item=operation>
            <div class="operation" data-code=operation.code>
                {{operation.name}}
            </div>
    </div>
    <div class="formula">

    </div>
</div>
